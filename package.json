{
    "name": "demoQATextBox",
    "version": "1.0.0",
    "description": "demoQATextBox for Soderberg & Partners",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --proxy-config proxy.conf.json --poll=5000",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "presentation-test": "protractor dist/out-tsc/test/resources/config/config.js --disableChecks",
        "presentation-test-build": "tsc --skipLibCheck",
        "presentation-test-clean": "rimraf dist/out-tsc/test/",
        "presentation-test-clean-build": "npm run presentation-test-clean && npm run presentation-test-build",
        "webdriver-update-local": "webdriver-manager update --versions.chrome=96.0.4664.45 ",
        "webdriver-start": "webdriver-manager start",
        "webdriver-clean": "webdriver-manager clean"
    },
    "dependencies": {
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cucumber": "^6.0.5",
        "dateformat": "^4.5.1",
        "faker": "^4.1.0",
        "protractor-cucumber-framework": "^6.2.0",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@angular/cli": "^1.7.4",
        "@types/node": "^6.0.118",
        "multiple-cucumber-html-reporter": "^1.13.1",
        "protractor": "~5.1.2",
        "typescript": "^3.2.4"
    }
}
